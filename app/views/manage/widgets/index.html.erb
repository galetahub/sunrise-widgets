<div class="duo-bl">
  <div class="left-part">
    <div class="content">
    	<div class="row-container">
        <div class="white-row">
          <div class="r-corn">
            <%= link_to t('manage.menu.widgets'), manage_widgets_path, :class=>"dark-text" %>
            <div class="act-bl">
              <%= link_to t('manage.add'), new_manage_widget_path, :class=>"create" %>
            </div>
          </div>
        </div>
      </div>
      <div id="widgets" class="stage">
        <%= render :partial=>"manage/widgets/widget", :collection=>@widgets %>
        <%= paginate @widgets, :theme => 'manage' %>
      </div>
      <script type='text/javascript'>
        $(document).ready(function(){
          Manage.init_collection('widgets', 'dinamic-bl');
        });
      </script>
    </div>
  </div>
  <div class="right-part">
    <div class="filter-right">
      <%= render :partial=>"manage/widgets/model_filter" %>
    </div>
  </div>
</div> 
